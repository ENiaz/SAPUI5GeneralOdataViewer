				</smartTable:customToolbar>-->
				<m:Table id="tableDataSet" mode="MultiSelect" busy="{detailView>/tableSectionTypeBusy}">
					<m:columns>
						<m:Column visible="true">
							<m:customData>
								<core:CustomData key="p13nData"
									value='\{"sortProperty": "__metadata/type", "filterProperty": "__metadata/type", "columnKey": "__metadata/type", "leadingProperty": "__metadata/type", "columnIndex":"0"}'/>
							</m:customData>
							<m:Text text="{detailView>/__metadata/type}"/>
						</m:Column>
						<m:Column visible="false">
							<m:customData>
								<core:CustomData key="p13nData"
									value='\{"sortProperty": "__metadata/uri", "filterProperty": "__metadata/uri", "columnKey": "__metadata/uri", "leadingProperty": "__metadata/uri", "columnIndex":"1"}'/>
							</m:customData>
							<m:Text text="{detailView>/__metadata/uri}"/>
						</m:Column>
					</m:columns>
					<m:items>
						<m:ColumnListItem type="Inactive" press="onTableItemPress">
							<m:cells>
								<m:Text text="{path:'DataModel>__metadata/type'}"/>
								<m:Text text="{path:'DataModel>__metadata/uri'}"/>
							</m:cells>
						</m:ColumnListItem>
					</m:items>
				</m:Table>
			</smartTable:SmartTable>
</core:FragmentDefinition>	